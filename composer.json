{
    "name": "encoredigitalgroup/stdlib",
    "description": "A collection of standard library classes and functions for PHP.",
    "type": "library",
    "license": "proprietary",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "php": "^8.2",
        "brick/std": "0.4.0",
        "encoredigitalgroup/foundation": "^1.3",
        "illuminate/collections": "^10|^11",
        "illuminate/support": "^10|^11",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^1.0",
        "tightenco/duster": "^2.7",
        "tomasvotruba/cognitive-complexity": "^0.2.3",
        "tomasvotruba/unused-public": "^0.3.9"
    },
    "autoload": {
        "psr-4": {
            "EncoreDigitalGroup\\StdLib\\": "src/"
        },
        "files": [
            "src/ObjectHelpers/dir_helpers.php",
            "src/ObjectHelpers/file_helpers.php",
            "src/ObjectHelpers/num_helpers.php",
            "src/ObjectHelpers/php_function_helpers.php",
            "src/ObjectHelpers/php_helpers.php",
            "src/ObjectHelpers/str_helpers.php",
            "src/ObjectHelpers/url_helpers.php",
            "src/ObjectHelpers/val_helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {}
}
